<launch>
  <arg name="KINECT1" default="$(optenv KINECT1 false)" />
  <arg name="KINECT2" default="$(optenv KINECT2 false)" />

  <!-- Startup PR2 without any mechanism controllers -->
  <include file="$(find modulation_rl)/robots_world/pr2/pr2_no_controllers.launch" pass_all_args="true"/>

  <!-- Load and Start Default Controllers -->
  <include file="$(find modulation_rl)/robots_world/pr2/pr2_default_controllers.launch" />

</launch>

